<h1>Edit Educational Information</h1>
<%= form_for @employee, url: update_educational_info_employee_path(@employee), method: :patch do |f| %>
  <%= f.label :field_of_study %>
  <%= f.text_field :field_of_study %>

  <%= f.label :university %>
  <%= f.text_field :university %>

  <%= f.label :graduation_year %>
  <%= f.number_field :graduation_year %>

  <%= f.label :gpa %>
  <%= f.number_field :gpa %>

  <%= f.label :additional_degrees %>
  <%= f.text_field :additional_degrees %>

  <%= f.submit "Update Educational Info" %>
<% end %>
